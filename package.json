{
  "name": "screen-server",
  "version": "1.1.0",
  "description": "screen-server",
  "main": "main.js",
  "build": {
    "appId": "com.screen-server.app",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "scripts": {
    "start": "electron-packager . screen-server --win --out build --arch=x64 --electron-version=4.1.5 --overwrite --ignore=.idea",
    "dist": "electron-builder --win --x64",
    "dist32": "electron-builder --win --ia32"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "getmac": "^1.4.6",
    "ws": "^7.0.1"
  }
}
